---
import { myProjects } from "@/data/projects/myProjects.data";

import ProjectCard from "@/components/shared/project/ProjectCard.astro";
---

<div class="grid items-stretch grid-cols-1 md:grid-cols-2 gap-8 md:gap-6 mt-12">
    {
        myProjects.map( project => (
            <ProjectCard project={project} />
        ))
    }
</div>